<%if(sucess && sucess.length){%><!--checking that the arry of sucess messge should not empty-->
    <div class="alert alert-success alert-dismissible fade show col-6b offset-3" role="alert" > <%=sucess %> <!--Displaying the error mesage by sucess pass by locals-->
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>    
    </div>
<%}%>



<%if(error && error.length){%><!--checking that the arry of sucess messge should not empty-->
    <div class="alert alert-danger alert-dismissible fade show col-6b offset-3" role="alert" > <%=error %> <!--Displaying the error mesage by sucess pass by locals-->
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>    
    </div>
<%}%>



